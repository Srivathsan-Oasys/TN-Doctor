<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDefaultResource">

    <data>

        <variable
            name="viewModel"
            type="com.oasys.digihealth.doctor.ui.quick_reg.view_model.LabTestProcessViewModel" />
    </data>

    <androidx.drawerlayout.widget.DrawerLayout
        android:id="@+id/drawerLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="false"
        tools:openDrawer="end">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/mainLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/llCenter"
                    android:orientation="vertical"
                    android:weightSum="5">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_toStartOf="@+id/searchDrawerCardView"
                            android:fontFamily="@font/poppins_bold"
                            android:paddingStart="6dp"
                            android:text="Test Process"
                            android:layout_marginTop="16dp"
                            android:textColor="@color/hmis_background"
                            android:textSize="@dimen/dr_title_text" />

                        <androidx.cardview.widget.CardView
                            android:id="@+id/searchDrawerCardView"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="16dp"
                            android:clickable="true"
                            android:focusable="true"
                            android:foreground="?attr/selectableItemBackgroundBorderless"
                            app:cardBackgroundColor="@color/colorPrimary"
                            app:cardCornerRadius="22dp">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                app:srcCompat="@drawable/ic_favourite_back_arrow"
                                tools:ignore="ContentDescription" />
                        </androidx.cardview.widget.CardView>


                    </RelativeLayout>


                    <LinearLayout
                        android:id="@+id/above"
                        android:layout_width="match_parent"
                        android:layout_height="70dp"
                        android:layout_weight="0.20"
                        android:orientation="horizontal"
                        android:padding="10dp"
                        android:weightSum="4">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@color/red"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/positiveTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="Positive"
                                android:textColor="@color/white"
                                android:textSize="14sp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="1"
                            android:background="@color/secondaryColor"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/negativeTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="Negative"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="1"
                            android:background="@color/Skyblue"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/equivocalTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="Equivocal"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="1"
                            android:background="@color/grey"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/rejectedTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins"
                                android:text="Rejected"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:layout_weight="5"
                        android:background="@drawable/edittext_border_white"
                        android:orientation="vertical">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:background="@drawable/edittext_border_white"
                            android:orientation="horizontal"
                            android:weightSum="7.5">

                            <CheckBox
                                android:id="@+id/selectAllCheckBox"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="0.3"
                                android:textColor="@color/navColor">

                            </CheckBox>


                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.3"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Patient Info"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>

                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.8"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Order Number"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>

                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.40"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Test Name"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>


                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />


                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Date&amp;Time"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>


                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.50"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Test Method"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>

                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />


                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.7"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Status"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>

                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".5"
                                android:fontFamily="@font/poppins_semibold"
                                android:gravity="center"
                                android:text="Action"
                                android:textColor="@color/navColor"
                                android:textSize="15sp">

                            </TextView>

                            <View
                                android:layout_width="2dp"
                                android:layout_height="match_parent"
                                android:background="@color/view" />


                        </LinearLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/labTestProcessrecycleview"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginEnd="1dp"
                            android:scrollbars="horizontal"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llCenter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/bottom"
                    android:layout_marginBottom="8dp"
                    android:orientation="vertical">

                    <ProgressBar
                        android:id="@+id/progressbar"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-7dp"
                        android:backgroundTint="@color/red"
                        android:indeterminate="true"
                        android:indeterminateTint="#1a09d6"
                        android:max="100" />

                    <View
                        android:id="@+id/view"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/view" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="8dp"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/result"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        app:cardBackgroundColor="@color/colorPrimaryDark"
                        app:cardCornerRadius="8dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="10dp"
                                app:srcCompat="@drawable/ic_thumb"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/poppins"
                                android:gravity="center"
                                android:paddingEnd="@dimen/_5sdp"
                                android:text="Sample Recieve"
                                android:textColor="@color/white"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/order"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        app:cardBackgroundColor="@color/orange"
                        app:cardCornerRadius="8dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="10dp"
                                app:srcCompat="@drawable/ic_order_process"
                                tools:ignore="HardcodedText" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/poppins"
                                android:gravity="center"
                                android:paddingRight="@dimen/_5sdp"
                                android:text="Order to Process"
                                android:textColor="@color/white"
                                android:textSize="12sp" />

                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/saveOfApproval"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        app:cardBackgroundColor="@color/outPtientGreen"
                        app:cardCornerRadius="8dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="10dp"
                                app:srcCompat="@drawable/ic_tick_12"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/poppins"
                                android:gravity="center"
                                android:paddingRight="@dimen/_5sdp"
                                android:text="Send Approval"
                                android:textColor="@color/white"
                                android:textSize="12sp" />

                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/assign_others"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="2sp"
                        android:clickable="true"
                        android:focusable="true"
                        app:cardBackgroundColor="@color/colorPrimaryDark"
                        app:cardCornerRadius="8dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="10dp"
                                app:srcCompat="@drawable/ic_arrow_forward_white"
                                tools:ignore="HardcodedText" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/poppins"
                                android:gravity="center"
                                android:paddingRight="@dimen/_5sdp"
                                android:text="Assign others"
                                android:textColor="@color/white"
                                android:textSize="12sp" />

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/rejected"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        app:cardBackgroundColor="@color/red"
                        app:cardCornerRadius="8dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="10dp"
                                app:srcCompat="@drawable/ic_thumbdown"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/poppins"
                                android:gravity="center"
                                android:paddingRight="@dimen/_5sdp"
                                android:text="Reject"
                                android:textColor="@color/white"
                                android:textSize="12sp" />

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>
            </RelativeLayout>


            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginStart="325dp"
                android:layout_marginTop="40dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/colorPrimary"
                app:cardCornerRadius="22dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_favourite_back_arrow"
                    tools:ignore="ContentDescription" />
            </androidx.cardview.widget.CardView>


        </FrameLayout>


        <com.google.android.material.navigation.NavigationView
            android:id="@+id/navigationView"
            android:layout_width="300dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="@dimen/_40sdp">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:cardElevation="0dp"
                app:cardUseCompatPadding="true">

                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="16dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:drawableLeft="@drawable/ic_filter"
                            android:drawablePadding="5dp"
                            android:fontFamily="@font/poppins_semibold"
                            android:padding="5dp"
                            android:text="Filter"
                            android:textSize="21sp"
                            tools:ignore="HardcodedText" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">

                                <androidx.appcompat.widget.AppCompatSpinner
                                    android:id="@+id/test_spinner"
                                    style="@style/Widget.AppCompat.Spinner.Underlined"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/poppins"
                                    android:hint="Test"
                                    android:inputType="textNoSuggestions"
                                    android:maxLines="1"
                                    android:padding="10dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                    android:textSize="18sp" />
                            </com.google.android.material.textfield.TextInputLayout>


                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <EditText
                                    android:id="@+id/calendarEditText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:background="@drawable/edit_text_bottom_border"
                                    android:drawableRight="@drawable/ic_calendar"
                                    android:focusable="false"
                                    android:hint="Date Range"
                                    android:padding="10dp" />
                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="30dp">

                                <androidx.appcompat.widget.AppCompatSpinner
                                    android:id="@+id/assignedToSpinner"
                                    style="@style/Widget.AppCompat.Spinner.Underlined"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/poppins"
                                    android:hint="Test"
                                    android:inputType="textNoSuggestions"
                                    android:maxLines="1"
                                    android:padding="10dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                    android:textSize="18sp" />
                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                    android:id="@+id/searchUsingMobileNo"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/ic_search"
                                    android:fontFamily="@font/poppins"
                                    android:hint="PIN/mobile No"
                                    android:inputType="number"
                                    android:maxLines="1"
                                    android:padding="10dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                    android:id="@+id/searchOrderNumber"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/ic_search"
                                    android:fontFamily="@font/poppins"
                                    android:hint="Order Process"
                                    android:inputType="textNoSuggestions"
                                    android:maxLines="1"
                                    android:padding="10dp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
                            </com.google.android.material.textfield.TextInputLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:gravity="right"
                                android:orientation="horizontal"
                                android:paddingEnd="@dimen/_3sdp">

                                <androidx.cardview.widget.CardView
                                    android:id="@+id/searchButton"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:cardBackgroundColor="@color/Skyblue"
                                    app:cardCornerRadius="5dp">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:drawableStart="@drawable/ic_search_white"
                                        android:drawablePadding="10dp"
                                        android:fontFamily="@font/poppins"
                                        android:padding="5dp"
                                        android:text="Search"
                                        android:textColor="@color/white"
                                        android:textSize="16sp"
                                        tools:ignore="HardcodedText" />
                                </androidx.cardview.widget.CardView>

                                <androidx.cardview.widget.CardView
                                    android:id="@+id/clear"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    app:cardBackgroundColor="@color/primaryGrey"
                                    app:cardCornerRadius="5dp">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:drawableStart="@drawable/ic_eraser"
                                        android:drawablePadding="10dp"
                                        android:fontFamily="@font/poppins"
                                        android:padding="5dp"
                                        android:text="Clear"
                                        android:textColor="@color/white"
