<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDefaultResource">

    <data>

        <variable
            name="viewModel"
            type="com.oasys.digihealth.doctor.ui.emr_workflow.lab.view_model.ManageLabPrevLabViewModel" />
    </data>

    <LinearLayout
        android:id="@+id/mainLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">




        <androidx.cardview.widget.CardView
            android:id="@+id/manage"
            android:layout_width="@dimen/dialog_size"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@android:color/white"
            tools:ignore="MissingDefaultResource">

            <LinearLayout
                android:layout_width="@dimen/dialog_size"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:context=".MainActivity">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:background="@color/hmis_background"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Lab Result"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dr_title_text"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/closeImageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/ic_close_white"
                        tools:ignore="VectorDrawableCompat" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="5dp"
                    android:orientation="vertical">

                    <androidx.core.widget.NestedScrollView
                        android:id="@+id/nestedScrollView"
                        android:layout_below="@+id/favouriteDrawerCardView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">



                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">


                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:background="@color/white"
                                            android:orientation="vertical">

                                            <View
                                                android:layout_width="match_parent"
                                                android:layout_height="2dp"
                                                android:background="@color/view" />




                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="50dp"
                                                android:gravity="center_vertical"
                                                android:orientation="horizontal"
                                                android:layout_weight="6">

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />

                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_gravity="center"
                                                    android:layout_weight="0.30"
                                                    android:id="@+id/sNoTextView"
                                                    android:fontFamily="@font/poppins_bold"
                                                    android:gravity="center"
                                                    android:textColor="@color/black"
                                                    android:text="S.No"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    tools:ignore="HardcodedText" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />

                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_gravity="center"
                                                    android:layout_weight="1"
                                                    android:fontFamily="@font/poppins_bold"
                                                    android:gravity="center"
                                                    android:text="Observation"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    android:textColor="@color/black"
                                                    tools:ignore="HardcodedText" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />


                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_weight="1"
                                                    android:fontFamily="@font/poppins_bold"
                                                    android:text="Result"
                                                    android:gravity="center"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    android:textColor="@color/black"
                                                    tools:ignore="HardcodedText,RtlSymmetry" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />

                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_gravity="center"
                                                    android:layout_weight="1"
                                                    android:fontFamily="@font/poppins_bold"
                                                    android:gravity="center"
                                                    android:text="Uom"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    android:textColor="@color/black"
                                                    tools:ignore="HardcodedText" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />
                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_gravity="center"
                                                    android:layout_weight="1.70"
                                                    android:fontFamily="@font/poppins_semibold"
                                                    android:gravity="center"
                                                    android:text="Reference Range"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    android:textColor="@color/black"
                                                    tools:ignore="HardcodedText" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />


                                                <TextView
                                                    android:layout_width="0dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_weight="1"
                                                    android:fontFamily="@font/poppins_semibold"
                                                    android:text="@string/Action"
                                                    android:textSize="@dimen/dr_medium_text"
                                                    android:gravity="center"
                                                    android:textColor="@color/black"
                                                    tools:ignore="HardcodedText,RtlSymmetry" />

                                                <View
                                                    android:layout_width="2dp"
                                                    android:layout_height="match_parent"
                                                    android:background="@color/view" />


                                            </LinearLayout>
                                            <View
                                                android:layout_width="match_parent"
                                                android:layout_height="2dp"
                                                android:background="@color/view" />



                                            <androidx.recyclerview.widget.RecyclerView
                                                android:id="@+id/labManagePrevLabRecyclerView"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content" />                                        </LinearLayout>


                                </androidx.cardview.widget.CardView>

                                <androidx.cardview.widget.CardView
                                    android:id="@+id/cancelCardView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:elevation="5dp"
                                    android:layout_gravity="end"
                                    android:layout_marginTop="6dp"
                                    android:layout_marginEnd="8dp"
                                    app:cardBackgroundColor="@color/primaryGrey"
                                    app:cardCornerRadius="8dp"
                                    android:clickable="true"
                                    android:focusable="true">

                                    <TextView
                                        android:id="@+id/tv_cancel"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:drawableStart="@drawable/ic_close_white"
                                        android:drawablePadding="7dp"
                                        android:fontFamily="@font/poppins"
                                        android:padding="7dp"
                                        android:text="Cancel"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/dr_medium_text"
                                        tools:ignore="HardcodedText" />

                                </androidx.cardview.widget.CardView>
                            </LinearLayout>


                        </LinearLayout>
                    </androidx.core.widget.NestedScrollView>
                </LinearLayout>

            </LinearLayout>


        </androidx.cardview.widget.CardView>


    </LinearLayout>
</layout>