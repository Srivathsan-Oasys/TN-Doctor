<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDefaultResource">

    <data>

<!--        <variable-->
<!--            name="viewModel"-->
<!--            type="com.oasys.digihealth.doctor.ui.report.quick_reg_reports.departmentWiseReport.viewModel.DepartmentSummaryViewModel"/>-->
    </data>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >

        <LinearLayout
            android:id="@+id/mainLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/listLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:padding="@dimen/_5sdp"
                android:visibility="visible"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/dateLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:text="Start Date: "
                        android:fontFamily="@font/poppins_semibold"
                        android:id="@+id/startDateLable"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:id="@+id/startDate"
                        android:text=""
                        android:fontFamily="@font/poppins_semibold"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_marginLeft="20dp"
                        android:text="End Date: "
                        android:fontFamily="@font/poppins_semibold"
                        android:id="@+id/endDateLable"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:id="@+id/endDate"
                        android:text=""
                        android:fontFamily="@font/poppins_semibold"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/header_layout"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:background="@drawable/edittext_border_white"
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:weightSum="31">


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:fontFamily="@font/poppins_bold"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="@dimen/_5sdp"
                        android:text="Session"
                        android:textColor="@color/navColor"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>


                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="12"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_3sdp"
                            android:text="New"
                            android:textColor="@color/navColor"
                            android:textSize="@dimen/dr_medium_text" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/view" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="12">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="3.1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:padding="@dimen/_3sdp"
                                    android:text="Adult"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="M"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="F"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="TG"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="Total"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="3.8"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:padding="@dimen/_3sdp"
                                    android:text="Child"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="M"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="F"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="Total"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="5.1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:text="Total"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                            </LinearLayout>


                        </LinearLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="12"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_3sdp"
                            android:text="Old"
                            android:textColor="@color/navColor"
                            android:textSize="@dimen/dr_medium_text" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/view" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="12">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="3.1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:padding="@dimen/_3sdp"
                                    android:text="Adult"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="M"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="F"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="TG"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="Total"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="3.8"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:padding="@dimen/_3sdp"
                                    android:text="Child"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="M"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="F"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/poppins_bold"
                                        android:gravity="center"
                                        android:text="Total"
                                        android:textColor="@color/navColor"
                                        android:textSize="@dimen/dr_medium_text" />

                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/view" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="5.1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/poppins_bold"
                                    android:gravity="center"
                                    android:text="Total"
                                    android:textColor="@color/navColor"
                                    android:textSize="@dimen/dr_medium_text" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/view" />

                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>


                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:fontFamily="@font/poppins_bold"
                        android:gravity="center"
                        android:text="Grand Total"
                        android:textColor="@color/navColor"
                        android:textSize="@dimen/dr_medium_text">

                    </TextView>

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />


                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/sessionWiseRecyclerView"/>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:gravity="right"
                    android:orientation="horizontal"
                    >

                    <androidx.cardview.widget.CardView
                        android:id="@+id/pdfDownload"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        app:cardBackgroundColor="@color/outPtientGreen"
                        android:visibility="visible"
                        android:layout_gravity="center"
                        app:cardCornerRadius="8dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableStart="@drawable/ic_save_pdf"
                            android:drawablePadding="10dp"
                            android:fontFamily="@font/poppins"
                            android:gravity="center"
                            android:text="Pdf"
                            android:paddingTop="7dp"
                            android:paddingBottom="7dp"
                            android:paddingLeft="15dp"
                            android:paddingRight="15dp"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dr_medium_text"
                            tools:ignore="HardcodedText" />
                    </androidx.cardview.widget.CardView>


                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

</layout>
