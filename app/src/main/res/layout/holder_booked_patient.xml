<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/llBookedPatients"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="1500dp"
        android:layout_height="35dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientSno"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text=""
            android:textColor="@color/black"
            android:textSize="12sp" />

        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientPin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="5555555555"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="Test test"
            android:textColor="@color/black"
            android:textSize="12sp" />



        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientAge"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="25"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientGender"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="Male"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientMobile"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="9876543210"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvBookedPatientDateTime"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="2020-07-22 "
            android:textColor="@color/black"
            android:textSize="12sp" />


    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view" />

</androidx.appcompat.widget.LinearLayoutCompat>