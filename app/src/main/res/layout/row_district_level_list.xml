<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="700dp"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <HorizontalScrollView
        android:layout_width="700dp"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="700dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="700dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:id="@+id/mainLayout"
                android:background="@drawable/edittext_border_white"
                android:orientation="horizontal"
                android:weightSum="30.5">

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:id="@+id/sNoTxt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:id="@+id/wardNameTxt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:id="@+id/institutionTxt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:id="@+id/institutionTypeTxt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />



                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:weightSum="4"
                    android:layout_weight="7">

                    <TextView
                        android:id="@+id/maleAdultTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/femaleadultTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/transgenderadultTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/totaladultTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_weight="6"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/malechildTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".75"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/femalechildTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".75"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />


                    <TextView
                        android:id="@+id/totalchildTxt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.50"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                </LinearLayout>

                <TextView
                    android:id="@+id/totalTxt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:fontFamily="@font/poppins"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="700dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:id="@+id/totalLayout"
                android:background="@drawable/edittext_border_white"
                android:orientation="horizontal"
                android:weightSum="30.5">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:text="Total"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="@dimen/_5sdp"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="12sp" />


                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:weightSum="4"
                    android:layout_weight="7">

                    <TextView
                        android:id="@+id/maleAdultTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/femaleadultTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/transgenderadultTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/totaladultTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_weight="6"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/malechildTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".75"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                    <TextView
                        android:id="@+id/femalechildTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".75"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />


                    <TextView
                        android:id="@+id/totalchildTxtTotal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.50"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/view" />

                </LinearLayout>

                <TextView
                    android:id="@+id/txtgrandTotal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:fontFamily="@font/poppins"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="@color/view" />

            </LinearLayout>

        </LinearLayout>

    </HorizontalScrollView>
</LinearLayout>
